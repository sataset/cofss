#set terminal gif animate delay 100
#set output 'foobar.gif'
set datafile separator ", "
stats "freq_logs.csv"
c = 299792.458
lambda = 1885.0
shift = c / lambda
#shift = 159.0410917772
pi = 3.1415926536

set title 'Frequency domain'

do for [j=0:STATS_blocks] {
plot "freq_logs.csv" index j u (c / ($1/2/pi + shift)):2 w lines title 'Right Polarization' , \
"freq_logs.csv" index j u (c / ($1/2/pi + shift)):3 w lines title 'Left Polarization'
pause -1
}
