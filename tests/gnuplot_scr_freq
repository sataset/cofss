set datafile separator ", "

set title 'Frequency domain'

plot "freq_logs.csv" index 0 u 1:2 w lines title 'Right Polarization' , \
"freq_logs.csv" index 0 u 1:3 w lines title 'Left Polarization'
pause -1
plot "freq_logs.csv" index 1 u 1:2 w lines title 'Right Polarization' , \
"freq_logs.csv" index 1 u 1:3 w lines title 'Left Polarization'
pause -1

