set datafile separator ", "
file = "logs/time_logs.csv"
stats file

set title 'Time domain'
#set xrange [30:70]

do for [j=0:STATS_blocks] {
plot file index j u 1:(sqrt($2*$2 + $3*$3)) w lines title 'Right Polarization' , \
file index j u 1:(sqrt($4*$4 + $5*$5)) w lines title 'Left Polarization'
pause -1
}
