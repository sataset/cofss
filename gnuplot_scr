set datafile separator ", "

plot "logs.csv" index 0 u 1:(sqrt($2*$2 + $3*$3)) w lines , \
"logs.csv" index 0 u 1:(sqrt($4*$4 + $5*$5)) w lines
pause -1
plot "logs.csv" index 1 u 1:(sqrt($2*$2 + $3*$3)) w lines , \
"logs.csv" index 1 u 1:(sqrt($4*$4 + $5*$5)) w lines
pause -1

